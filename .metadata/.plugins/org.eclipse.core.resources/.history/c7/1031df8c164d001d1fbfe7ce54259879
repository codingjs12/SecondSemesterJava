
public class Song {
String title;
	public song(String title) {
		this.title=title;
		
	}
	public String getTitle() {
		return this.title;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Song mySong = new Song("Nessun Dorma");
		Song yourSong = new Song("공주는 잠못이루고");
	}

}
