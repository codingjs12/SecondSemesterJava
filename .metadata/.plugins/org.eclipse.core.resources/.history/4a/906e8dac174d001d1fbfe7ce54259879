class Car {
	String color;
	String gearType;
	int door;
	

Car(String color, String gearType, int door) {
	this.color = color;
	this.gearType = gearType;
	this.door = door;
	
}
Car() {
	color = "white";
	gearType = "auto";
	door = 4;
}


public class Song {
Car myCar = new Car();

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print(Car.Song.class);
	}

}
}
